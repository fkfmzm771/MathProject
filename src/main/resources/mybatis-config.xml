<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<typeAliases>
<!--		게임 VO-->
		<typeAlias type="Math.Spring.com.VO.GameCard" alias="GameCard"/>
		<typeAlias type="Math.Spring.com.VO.GameScore" alias="GameScore"/>
		<typeAlias type="Math.Spring.com.VO.GameStage"  alias="GameStage"/>

<!--		유저 정보-->
		<typeAlias type="Math.Spring.com.VO.Student" alias="Student"/>
		<typeAlias type="Math.Spring.com.VO.Parents" alias="Parents"/>
		<typeAlias type="Math.Spring.com.VO.Teacher" alias="Teacher"/>
		<typeAlias type="Math.Spring.com.VO.Friend" alias="Friend"/>
		<typeAlias type="Math.Spring.com.VO.Myclass" alias="Myclass"/>
		<typeAlias type="Math.Spring.com.VO.MathBoard"  alias="MathBoard"/>
	</typeAliases>
	<mappers>
		<mapper resource="mappers/chartMapper.xml"></mapper>
		<mapper resource="mappers/UserMapper.xml"></mapper>
		<mapper resource="mappers/FriendMapper.xml"></mapper>
		<mapper resource="mappers/MyClassMapper.xml"></mapper>
		<mapper resource="mappers/FamilyMapper.xml"></mapper>
		<mapper resource="mappers/MathBoardMapper.xml"></mapper>
		<mapper resource="mappers/GameMapper.xml"></mapper>
	</mappers>
</configuration>

